class Person:
    def __init__(self, name, age, gender):
        self.name = name
        self.age = age
        self.gender = gender
    
    def getName(self):
        return self.name
    def getAge(self):
        return self.age
    def getGender(self):
        return self.gender


class Student(Person):
    def __init__(self, name, age, gender):
        super().__init__(name, age, gender)
    

s1 = Student("Angelica", 19, "Female")

print(s1.getName())
print(s1.getAge())
print(s1.getGender())
